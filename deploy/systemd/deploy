#/bin/sh
ls -lh
scp -o "StrictHostKeyChecking no" -i $TREESQL_DEPLOY_KEY ../../treesql-server-linux $TREESQL_DEPLOY_HOST:treesql-server
scp -o "StrictHostKeyChecking no" -i $TREESQL_DEPLOY_KEY ran $TREESQL_DEPLOY_HOST:ran
scp -o "StrictHostKeyChecking no" -i $TREESQL_DEPLOY_KEY -r ../../webui/build $TREESQL_DEPLOY_HOST:webui
./restart
